<div class="row">
  <div class="col-md-6 offset-md-3">
    <table class="table table-sm">
      <thead>
        <tr>
          <th>Name</th>

        </tr>
      </thead>

      <tbody>
        <% @ayudantes.each do |ayudante| %>
          <tr>
            <td><%= User.find(ayudante.user_id).email %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <button type="button" class="btn btn-outline-success">
      <%= link_to 'Back to Courses', inscriptions_courses_path %>
    </button>
  </div>
</div>
